                <h3 style="font-weight: 100; margin-left: 20px; margin-top: 40px;"><b style="color: #000;font-size: 16px;">（九）其他需说明的事项</b></h3>
                <div style="border: 1px solid #e5e1e1;margin: 0 30px;">
                    <div style="padding: 10px 0; padding-left: 170px; line-height: 26px; min-height: 26px; word-wrap:break-word; margin-top: 30px">
                        <label style="position: absolute; margin-left: -170px; width: 170px; text-align: right;">其他事项说明：</label>
                        <div>
                            <p style="line-height: 24px; text-indent: 2em;padding-right: 30px;">$!{contentInfo.otherExplain.replaceAll("\n","</br>").replaceAll(" ","&nbsp;")}</p>
                        </div>
                    </div>                 
                </div>
            </div>

            <div class="m-invn" style="color: #666">
                <h3 style="font-weight: 100; margin-left: 20px; margin-top: 40px;"><b style="color: #000;font-size: 16px;">（十）分析及结论（客户对外担保风险较大的需对被担保方进行分析）</b></h3>
                <div style="border: 1px solid #e5e1e1;margin: 0 30px;">
                    <div style="padding: 10px 0; padding-left: 170px; line-height: 26px; min-height: 26px; word-wrap:break-word; margin-top: 30px">
                        <label style="position: absolute; margin-left: -170px; width: 170px; text-align: right;">分析及结论：</label>
                        <div>
                            <p style="line-height: 24px; text-indent: 2em;padding-right: 30px;">$!{contentInfo.analysisConclusion.replaceAll("\n","</br>").replaceAll(" ","&nbsp;")}</p>
                        </div>
                    </div>                 
                </div>
            </div>

            <div class="m-invn" style="color: #666">
                <h3 style="font-weight: 100; margin-left: 20px; margin-top: 40px;"><b style="color: #000;font-size: 16px;">（十一）附件上传</b></h3>
                #set($attachmentTitle="相关附件")
                #set($attachmentNameAttr="pathName_CONTENT_ATTACHMENT")
                #set($hiddenUrls = "")
                #set($hiddenUrls = $!{hiddenUrls_CONTENT_ATTACHMENT})
                #set($attachments = "")
                #set($attachments = $!{attaches_CONTENT_ATTACHMENT})
                <input class="fnUpAttachVal" type="hidden" name="$!attachmentNameAttr" value="$!hiddenUrls">
                <div class="m-attach fnUpAttachUl">
                    #foreach($item in $!{attachments})
                        #if($!item.fileName.endsWith(".jpg") || $!item.fileName.endsWith(".JPG") ||
                            $!item.fileName.endsWith(".png") || $!item.fileName.endsWith(".PNG") ||
                            $!item.fileName.endsWith(".jpeg") || $!item.fileName.endsWith(".JPEG") ||
                            $!item.fileName.endsWith(".tiff") || $!item.fileName.endsWith(".TIFF") ||
                            $!item.fileName.endsWith(".bmp") || $!item.fileName.endsWith(".BMP"))
                        #set($fullStr = $!item.fileName + "," + $!item.filePhysicalPath + "," + $!item.requestPath)
                            <span class="attach-item fnAttachItem" val="$!fullStr">
                                <span class="fnAttachView fn-csp"><i class="icon icon-img"></i>$!{item.fileName}</span>
                            </span>
                        #else
                            <a title="点击下载" href="/baseDataLoad/downLoad.htm?id=$!item.attachmentId">
                                <span style="position: relative;padding: 5px;padding-right: 25px;margin: 3px;color: #000;border: 1px solid #666;background-color: #f5f5f5;border-radius: 4px;">
                                    <i class="icon icon-file"></i>
                                    $!{item.fileName}
                                </span>
                            </a>
                        #end
                    #end
                </div>
    			#parse("/projectMg/afterLoanMg/afterConfirminSpect/exportCommonContent.vm")
            </div>
        </div>
        <div id="btn" style="position: fixed; right: 0;top:50%; background:#e15450;color:#fff;cursor: pointer;padding: 5px;border-radius: 5px;">导出</div>

        #parse("/projectMg/afterLoanMg/afterConfirminSpect/commonApprovalRecords.vm")

        #parse("/layout/common/commonjs.vm")
        <script>
            seajs.use($_GLOBAL.zyw+'/project/bfcg.itn.edit.toExportDoc');
        </script>
    </body>
</html>