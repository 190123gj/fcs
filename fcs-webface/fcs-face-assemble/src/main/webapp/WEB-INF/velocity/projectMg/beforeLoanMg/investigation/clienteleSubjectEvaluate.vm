<!DOCTYPE html>
<html lang="zh-cmn-Hans">
<head>
#parse("/layout/common/head.vm")
<link type="text/css" href="/styles/tmbp/project2.css" rel="stylesheet">
<title>担保/委贷业务项目调查（客户主体评价）</title>
</head>
<body>

<div class="ui-breadcrumb">
    &nbsp;项目管理&nbsp;&gt;&nbsp;授信前管理&nbsp;&gt;&nbsp;担保/委贷业务项目调查（客户主体评价）
</div>


<div class="fn-blank20"></div>

<div class="m-main border ui-bg-fff">

    #parse("/layout/projectMg/beforeLoanMg/investigation/tabNav.vm")

    <form class="fnStep" action="/projectMg/investigation/saveInvestigationMainlyReviewSubmit.htm" method="post" id="form">
		#parse("/projectMg/beforeLoanMg/investigation/investigationCommon.vm")
		#if($!{info})
    	<input type="hidden" name="MReviewId" value="$!{info.MReviewId}" />
		#end

        <h2 class="fn-tac apply-h2 fn-mt45">二、客户主体评价</h2>
        <div class="m-blank fn-mt20 fn-mb20"></div>
        <div class="m-invn">
            <h3 class="m-invndt fn-mt40 fn-ml20"><b>（一）客户基本情况</b></h3>
            <dl class="fn-mt20">
                <dd class="fn-p-reb">
                    <table class="m-table" border="1">
                        <tbody>
                            <tr>
								#set($establishedTime = $!{info.establishedTime})
								#if($!{customerBaseInfo})
									#set($establishedTime = $!{customerBaseInfo.establishedTime})
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>成立时间</td>
                                <td><input class="text" type="text" name="establishedTimeStr" value="$dateUtil.dtSimpleFormat($!{establishedTime})" readonly></td>
                                <td class="fn-font-b2"><em class="remind">*</em>经营期限</td>
                                <td><input class="text" type="text" name="operatingTerm" value="$!{info.operatingTerm}"></td>
                            </tr>
                            <tr>
								#set($legalPersion = $!{info.legalPersion})
								#if($!{customerBaseInfo})
									#set($legalPersion = $!{customerBaseInfo.legalPersion})
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>法定代表人</td>
                                <td><input class="text" type="text" name="legalPersion" value="$!{legalPersion}" readonly></td>
								#set($livingAddress = $!{info.livingAddress})
								#if($!{customerBaseInfo} && $!{customerBaseInfo.legalPersionAddress} && $!{customerBaseInfo.legalPersionAddress}!="")
									#set($livingAddress = $!{customerBaseInfo.legalPersionAddress})
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>住所</td>
                                <td><input class="text" type="text" name="livingAddress" value="$!{livingAddress}" ></td>
                            </tr>
                            <tr>
                                <td class="fn-font-b2"><em class="remind">*</em>实际控制人</td>
                                <td><input class="text" type="text" name="actualControlPerson" value="$!{info.actualControlPerson}"></td>
								#set($busiLicenseNo = $!{info.busiLicenseNo})
								#if($!{customerBaseInfo})
									#set($busiLicenseNo = $!{customerBaseInfo.busiLicenseNo})
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>营业执照号</td>
                                <td><input class="text" type="text" name="busiLicenseNo" value="$!{busiLicenseNo}" #if($!{busiLicenseNo}) readOnly #end ></td>
                            </tr>
                            <tr>
								#set($type = $!{info.type})
								#if($!{customerBaseInfo})
									#set($type = $!{customerBaseInfo.type})
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>企业类型</td>
                                <td>
                                    <input class="text" type="hidden" name="enterpriseType" value="$!{customerBaseInfo.enterpriseType.code}">
                                    <input class="text" type="text" name="enterpriseName" value="$!{customerBaseInfo.enterpriseType.message}" readOnly>
                                </td>
								#set($workAddress = $!{info.workAddress})
								#if($!{customerBaseInfo})
									#set($workAddress = $!{customerBaseInfo.address})
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>办公地址</td>
                                <td><input class="text" type="text" name="workAddress" value="$!{workAddress}"></td>
                            </tr>
							#if($!{customerBaseInfo.isOneCert.code}!="IS")
                            <tr>
                                <td class="fn-font-b2"><em class="remind">*</em>组织机构代码证</td>
								#set($orgCode = $!{info.orgCode})
								#if($!{customerBaseInfo} && $!{customerBaseInfo.orgCode} && $!{customerBaseInfo.orgCode}!="")
									#set($orgCode = $!{customerBaseInfo.orgCode})
                                <td><input class="text" type="text" name="orgCode" value="$!{orgCode}" readOnly></td>
								#else
                                <td><input class="text" type="text" name="orgCode" value="$!{orgCode}"></td>
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>税务登记证号</td>
								#set($taxCertificateNo = $!{info.taxCertificateNo})
								#if($!{customerBaseInfo} && $!{customerBaseInfo.taxCertificateNo} && $!{customerBaseInfo.taxCertificateNo}!="")
									#set($taxCertificateNo = $!{customerBaseInfo.taxCertificateNo})
                                <td><input class="text" type="text" name="taxCertificateNo" value="$!{taxCertificateNo}" readOnly></td>
								#else
                                <td><input class="text" type="text" name="taxCertificateNo" value="$!{taxCertificateNo}"></td>
								#end
                            </tr>
							#end
                            <tr>
                                <td class="fn-font-b2">贷款卡号</td>
                                <td><input class="text" type="text" name="loanCardNo" value="$!{info.loanCardNo}"></td>
								#set($busiScope = $!{info.busiScope})
								#if($!{customerBaseInfo})
									#set($busiScope = $!{customerBaseInfo.busiScope})
								#end
                                <td class="fn-font-b2"><em class="remind">*</em>业务范围</td>
                                <td><input class="text" type="text" name="busiScope" value="$!{busiScope}"></td>
                            </tr>
                            <tr>
                                <td class="fn-font-b2"><em class="remind">*</em>最后年检年度</td>
                                <td>
                                    <select class="text" name="lastCheckYear">
                                        <option value="">请选择</option>
										#foreach($year in $!{years})
                                        <option value="$!{year}" #if($!{year}==$!{info.lastCheckYear}) selected="selected" #end >$!{year}</option>
										#end
                                    </select>
                                    ##<input type="hidden" name="lastCheckYear" value="$!{info.lastCheckYear}">
                                </td>
                                <td></td>
                                <td></td>
                            </tr>
                        </tbody>
                    </table>
                </dd>
            </dl>
            <dl>
                <dt class="fn-pl30 fn-lh60 fn-mt20">已获得的资质证书</dt>
                <dd class="fn-p-reb">
                    <table class="m-table" border="1">
                        <thead>
                            <tr>
                                <th>资质证书名称</th>
                                <th>编码</th>
                                <th>有效期</th>
                                <th>操作</th>
                            </tr>
                        </thead>
                        <tbody id="test2">
							#if($listUtil.isNotEmpty($!{info.certificates}))
								#set($index = 1)
								#foreach($item in $!{info.certificates})
                                    <tr #if($index!=1) class="fnNewLine" #end orderName="certificates">
                                        <td><input class="text" type="text" name="certificateName" value="$!{item.certificateName}"></td>
                                        <td><input class="text" type="text" name="certificateCode" value="$!{item.certificateCode}"></td>
                                        <td><input class="text laydate-icon" type="text" name="validDateStr" onclick="var sel = this; laydate({choose: function(dates){$(sel).blur()}});" value="$dateUtil.dtSimpleFormat($!{item.validDate})"></td>
    									#if($index == 1)
                                        <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
    									#else
                                        <td class="fn-text-c"><a class="ui-btn ui-btn-fill ui-btn-green fnDelLine" parentsClass="fnNewLine"><i class="icon icon-del"></i>删除一行</a></td>
    									#end
                                    </tr>
    								#set($index = $index + 1)
								#end
							#else
                                <tr orderName="certificates">
                                    <td><input class="text" type="text" name="certificateName"></td>
                                    <td><input class="text" type="text" name="certificateCode"></td>
                                    <td><input class="text laydate-icon" type="text" name="validDateStr" onclick="var sel = this; laydate({choose: function(dates){$(sel).blur()}});"></td>
                                    <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
                                </tr>
							#end
                        </tbody>
                    </table>
                </dd>
            </dl>
            <a class="ui-btn ui-btn-fill ui-btn-green fnAddLine fn-ml30 fn-mt20" tplID="t-test2" cttID="test2"><i class="icon icon-add"></i>增加一行</a>
            <script type="text/html" id="t-test2">
                <tr class="fnNewLine" orderName="certificates">
                    <td><input class="text" type="text" name="certificateName"></td>
                    <td><input class="text" type="text" name="certificateCode"></td>
					<td><input class="text laydate-icon" type="text" name="validDateStr" onclick="var sel = this; laydate({choose: function(dates){$(sel).blur()}});"></td>
                    <td class="fn-text-c"><a class="ui-btn ui-btn-fill ui-btn-green fnDelLine" parentsClass="fnNewLine"><i class="icon icon-del"></i>删除一行</a></td>
                </tr>
            </script>
            <dl>
                <dt class="fn-pl30 fn-lh60 fn-mt20">开户情况</dt>
                <dd class="fn-p-reb">
                    <table class="m-table" border="1">
                        <tbody>
							#if($listUtil.isNotEmpty($!{info.banks}))
								#set($index = 0)
    							#foreach($bank in $!{info.banks})
                                <tr orderName="banks">
                                    <td class="fn-font-b2">#if($!{foreach.index}==0)<em class="remind">*</em>#end$!{bank.bankDesc}</td>
    								<input type="hidden" name="bankDesc" value="$!{bank.bankDesc}" />
                                    <td><input class="text" type="text" name="bankName" value="$!{bank.bankName}"></td>
                                    <td class="fn-font-b2">#if($!{foreach.index}==0)<em class="remind">*</em>#end账号</td>
                                    <td><input class="text" type="text" name="accountNo" value="$!{bank.accountNo}"></td>
                                </tr>
								#set($index = $index + 1)
								#end
							#else
                            <tr orderName="banks">
                                <td class="fn-font-b2"><em class="remind">*</em>基本账户开户行</td>
								<input type="hidden" name="bankDesc" value="基本账户开户行" />
                                <td><input class="text" type="text" name="bankName"></td>
                                <td class="fn-font-b2"><em class="remind">*</em>账号</td>
                                <td><input class="text" type="text" name="accountNo"></td>
                            </tr>
                            <tr orderName="banks">
                                <td class="fn-font-b2">主要结算账户开户行1</td>
								<input type="hidden" name="bankDesc" value="主要结算账户开户行1" />
                                <td><input class="text" type="text" name="bankName"></td>
                                <td class="fn-font-b2">账号</td>
                                <td><input class="text" type="text" name="accountNo"></td>
                            </tr>
                            <tr orderName="banks">
                                <td class="fn-font-b2">主要结算账户开户行2</td>
								<input type="hidden" name="bankDesc" value="主要结算账户开户行2" />
                                <td><input class="text" type="text" name="bankName"></td>
                                <td class="fn-font-b2">账号</td>
                                <td><input class="text" type="text" name="accountNo"></td>
                            </tr>
                            <tr orderName="banks">
                                <td class="fn-font-b2">其他结算账户开户行</td>
								<input type="hidden" name="bankDesc" value="其他结算账户开户行" />
                                <td><input class="text" type="text" name="bankName"></td>
                                <td class="fn-font-b2">账号</td>
                                <td><input class="text" type="text" name="accountNo"></td>
                            </tr>
							#end
                        </tbody>
                    </table>
                </dd>
            </dl>
        </div>
        <div class="m-invn fn-mt30">
            <h3 class="m-invndt fn-mt40 fn-ml20"><b>（二）客户发展沿革（背景）及重大机构变革</b></h3>
            <dl class="fn-mt20">
                <dd class="fn-p-reb">
                     <textarea class="ui-textarea2 fn-w500 fn-ml30" name="customerDevEvolution" placeholder="请控制在1000文字之内">$!{info.customerDevEvolution}</textarea>
                </dd>
            </dl>
        </div>
		#set($certTypes = [
			{"code":"ID_CARD", "name":"身份证"},
			{"code":"SOLDIERS", "name":"士兵证"},
			{"code":"OFFICER", "name":"军官证"},
			{"code":"POLICE_OFFICER", "name":"警官证"},
			{"code":"PASSPORT", "name":"护照"},
			{"code":"OTHER", "name":"其它"}
		])
		#set($marriages = ["未婚", "已婚", "离异", "丧偶"])
		#set($relations = ["实际控制人", "主要股东", "管理人员", "担保人"])
        <div class="m-invn fn-mt30">
            <h3 class="m-invndt fn-mt40 fn-ml20"><b>（三）客户隶属关系或控制关系情况</b></h3>
            <dl>
                <dt class="fn-pl30 fn-lh60 fn-mt20">1、主要股东情况表(提示：股东持比前四名必填)</dt>
            </dl>
            <dl id="test">
				#if($listUtil.isNotEmpty($!{info.holders}))
					#set($index = 1)
					#foreach($item in $!{info.holders})
						<dd class="fn-p-reb #if($index!=1) fnNewLine fn-mt45 #end " orderName="holders">
                            <table class="m-table" border="1">
                                <tbody class="shareholderNature">
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">股东性质</td>
                                        <td>
                                            <select class="text shareholderNatureSelect" name="holderTypeStr">
                                                #foreach($holderType in $!{typeList})
                                                    <option value="$!{holderType.code}" #if($!{holderType.code}==$!{item.holderType.code}) selected="selected" #end >$!{holderType.message}</option>
                                                #end
                                            </select>
                                            ##<input type="hidden" name="holderTypeStr" value="$!{item.holderType.code}"></td>
                                        <td class="fn-pl5 fn-font-b2">主要股东名称</td>
                                        <td><input class="text" type="text" name="holderName" value="$!{item.holderName}"></td>
                                    </tr>
                                </tbody>
                                <tbody class="shareholderNatureObj">
									#if($!{item.holderType.code}=="LEGAL_PERSON")
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">股东证件号码</td>
                                        <td><input class="text holderCertNoClass" type="text" name="legalCertno" value="$!{item.holderCertNo}"></td>
                                        <td class="fn-pl5 fn-font-b2">实际投资金额</td>
                                        <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="actualInvestmentStr" value="$!{item.actualInvestment}"><span>元</span></td>
                                    </tr>
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">占实收资本比例</td>
                                        <td class="m-invndd-font"><input class="text fnMakePercent100" type="text" name="paidinCapitalRatio" value="$rateUtil.formatRate($!{item.paidinCapitalRatio})"><span>%</span></td>
                                        <td class="fn-pl5 fn-font-b2">股东主营业务</td>
                                        <td><input class="text" type="text" name="holderMajorBusi" value="$!{item.holderMajorBusi}"></td>
                                    </tr>
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">股东规模（资产）</td>
                                        <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="capitalScaleStr" value="$!{item.capitalScale}"><span>元</span></td>
                                        <td class="fn-pl5 fn-font-b2">股东规模（收入）</td>
                                        <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="incomeScaleStr" value="$!{item.incomeScale}"><span>元</span></td>
                                    </tr>
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">股东规模（生产能力）</td>
                                        <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="produceScaleStr" value="$!{item.produceScale}"><span>元</span></td>
                                        <td></td>
                                        <td></td>
                                    </tr>
									#else
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">股东证件类型</td>
                                        <td>
                                            <select class="text credentialsType" name="holderCertType">
                                                <option value="">请选择股东证件类型</option>
												#foreach($cert in $certTypes)
                                                <option value="$!{cert.code}" #if($!{item.holderCertType}==$!{cert.code}) selected="selected" #end >$!{cert.name}</option>
												#end
                                            </select>
                                        </td>
                                        <td class="fn-pl5 fn-font-b2">证件号码</td>
                                        <td><input class="text" type="text" name="holderCertNo" value="$!{item.holderCertNo}"></td>
                                    </tr>
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">联系电话（手机）</td>
                                        <td><input class="text" type="text" name="holderContactNo" value="$!{item.holderContactNo}"></td>
                                        <td class="fn-pl5 fn-font-b2">婚姻状况</td>
                                        <td>
                                            <select class="text marriageChange" name="maritalStatus">
                                                <option value="">请选择婚姻状况</option>
												#foreach($marriage in $marriages)
                                                <option value="$!{marriage}" #if($!{item.maritalStatus}==$!{marriage}) selected="selected" #end>$!{marriage}</option>
												#end
                                            </select>
                                            ##<input class="text" type="hidden" name="maritalStatus" value="$!{item.maritalStatus}">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">配偶姓名</td>
                                        <td><input class="text" type="text" name="spouseName" value="$!{item.spouseName}"></td>
                                        <td class="fn-pl5 fn-font-b2">配偶联系电话（手机）</td>
                                        <td><input class="text" type="text" name="spouseContactNo" value="$!{item.spouseContactNo}"></td></td>
                                    </tr>
                                    <tr>
                                        <td class="fn-pl5 fn-font-b2">配偶证件类型</td>
                                        <td>
                                            <select class="text credentialsType" name="spouseCertType">
                                                <option value="">请选择配偶证件类型</option>
                                                #foreach($cert in $certTypes)
                                                <option value="$!{cert.code}" #if($!{item.spouseCertType}==$!{cert.code}) selected="selected" #end >$!{cert.name}</option>
                                                #end
                                            </select>
                                            ##<input type="hidden" name="spouseCertType">
                                        </td>
                                        <td class="fn-pl5 fn-font-b2">配偶证件号码</td>
                                        <td><input class="text" type="text" name="spouseCertNo" value="$!{item.spouseCertNo}"></td>
                                    </tr>
									#end
                                </tbody>
                            </table>
    						#if($index != 1)
							<a class="ui-btn ui-btn-fill ui-btn-green fn-p-abs fn-tr fnDelLine" parentsClass="fnNewLine"><i class="icon icon-del"></i>删除一项</a>
    						#end
                        </dd>
						#set($index = $index + 1)
					#end
				#else
                <dd class="fn-p-reb" orderName="holders">
                    <table class="m-table" border="1">
                        <tbody class="shareholderNature">
                            <tr>
                                <td class="fn-pl5 fn-font-b2">股东性质</td>
                                <td>
                                    <select class="text shareholderNatureSelect" name="holderTypeStr">
                                        #foreach($holderType in $!{typeList})
                                            <option value="$!{holderType.code}" #if($!{holderType.code}=="LEGAL_PERSON") selected="selected" #end >$!{holderType.message}</option>
                                        #end
                                    </select>
                                    ##<input type="hidden" name="holderTypeStr" value="LEGAL_PERSON">
								</td>
                                <td class="fn-pl5 fn-font-b2">主要股东名称</td>
                                <td><input class="text" type="text" name="holderName"></td>
                            </tr>
                        </tbody>
                        <tbody class="shareholderNatureObj">
                            <tr>
                                <td class="fn-pl5 fn-font-b2">股东证件号码</td>
                                <td><input class="text holderCertNoClass" type="text" name="legalCertno" value="$!{item.holderCertNo}"></td>
                                <td class="fn-pl5 fn-font-b2">实际投资金额</td>
                                <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="actualInvestmentStr" value="$!{item.actualInvestment}"><span>元</span></td>
                            </tr>
                            <tr>
                                <td class="fn-pl5 fn-font-b2">占实收资本比例</td>
                                <td class="m-invndd-font"><input class="text fnMakePercent100" type="text" name="paidinCapitalRatio" value="$rateUtil.formatRate($!{item.paidinCapitalRatio})"><span>%</span></td>
                                <td class="fn-pl5 fn-font-b2">股东主营业务</td>
                                <td><input class="text" type="text" name="holderMajorBusi" value="$!{item.holderMajorBusi}"></td>
                            </tr>
                            <tr>
                                <td class="fn-pl5 fn-font-b2">股东规模（资产）</td>
                                <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="capitalScaleStr" value="$!{item.capitalScale}"><span>元</span></td>
                                <td class="fn-pl5 fn-font-b2">股东规模（收入）</td>
                                <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="incomeScaleStr" value="$!{item.incomeScale}"><span>元</span></td>
                            </tr>
                            <tr>
                                <td class="fn-pl5 fn-font-b2">股东规模（生产能力）</td>
                                <td class="m-invndd-font"><input class="text fnMakeMoney" type="text" name="produceScaleStr" value="$!{item.produceScale}"><span>元</span></td>
                                <td></td>
                                <td></td>
                            </tr>
                        </tbody>
                    </table>
                </dd>
				#end
