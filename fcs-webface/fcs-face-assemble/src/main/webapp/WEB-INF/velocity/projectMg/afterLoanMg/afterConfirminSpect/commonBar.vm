<div class="fn-mb20 apply-step fn-pr" id="step">
    <ul class="ul fn-clear">
        <li class="item icon-s1 active" ftf="xmjbqk">
            <i class="icon icon-s1"></i>
            <p>项目基本情况</p>
            <div class="step-line"></div>
            <div class="step-line2"></div>
        </li>
        <li class="item icon-s2" ftf="jgnr">
            <i class="icon icon-s2"></i>
            <p>监管内容</p>
            <div class="step-line"></div>
            <div class="step-line2"></div>
        </li>
        #if($!showCustomer)
            <li class="item icon-s3" ftf="khzl">
                <i class="icon icon-s3"></i>
                <p>客户资料</p>
                <div class="step-line"></div>
                <div class="step-line2"></div>
            </li>
        #end
    </ul>
    <span class="Spectbb">$!{baseInfo.customerName}<input class="ui-text fn-w40 fnMakeNumber" goal="fnReportDateYear" type="text" value="$!{afterCheckInfo.roundYear}">年第<input class="ui-text fn-w40 fnMakeNumber" goal="fnReportDatePeriod" type=text"" value="$!{afterCheckInfo.roundTime}">期保后检查报告</span>
</div>