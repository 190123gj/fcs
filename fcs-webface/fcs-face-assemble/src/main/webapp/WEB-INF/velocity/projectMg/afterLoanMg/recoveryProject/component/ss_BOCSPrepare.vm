## 庭前准备 trailInfo

<div class="fn-ml20 fn-mr20">
	
    <input type="hidden" value="$!trailInfo.id" name="id">
    
    <table class="m-table m-table-list">

        <col width="150px">
        <col>
        <col width="120px">
        <col width="150px">
        <col>
        <col width="120px">

        <tbody>
            
            <tr>
                <td class="fn-tac fn-fwb"><span class="fn-f30 fn-mr5">*</span>开庭时间</td>
                <td><input type="text" class="text laydate-icon fnInput fnOpeningTime" name="openingTime" value="$!dateUtil.formatDay($!trailInfo.openingTime)" onclick="laydate()"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailOpeningTimeUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailOpeningTimeUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailOpeningTimeUrl</td>
                <td class="fn-tac fn-fwb">公告时间</td>
                <td><input type="text" class="text laydate-icon" name="noticeTime" value="$!dateUtil.formatDay($!trailInfo.noticeTime)" onclick="laydate()"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailNoticeTimeUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailNoticeTimeUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailNoticeTimeUrl</td>
            </tr>

            <tr>
                <td class="fn-tac fn-fwb">文书送达时间</td>
                <td><input type="text" class="text laydate-icon" name="clerkArrivedTime" value="$!dateUtil.formatDay($!trailInfo.clerkArrivedTime)" onclick="laydate()"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailClerkArrivedTimeUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailClerkArrivedTimeUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailClerkArrivedTimeUrl</td>
                <td class="fn-tac fn-fwb">管辖异议</td>
                <td><input type="text" class="text" name="jurisdictionObjection" value="$!trailInfo.jurisdictionObjection" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailJurisdictionObjectionUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionUrl</td>
            </tr>

            <tr>
                <td class="fn-tac fn-fwb">管辖异议裁定</td>
                <td><input type="text" class="text" name="jurisdictionObjectionJudgment" value="$!trailInfo.jurisdictionObjectionJudgment" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailJurisdictionObjectionJudgmentUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionJudgmentUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionJudgmentUrl</td>
                <td class="fn-tac fn-fwb">管辖异议上诉</td>
                <td><input type="text" class="text" name="jurisdictionObjectionAppeal" value="$!trailInfo.jurisdictionObjectionAppeal" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailJurisdictionObjectionAppealUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionAppealUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件  $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionAppealUrl</td>
            </tr>

            <tr>
                <td class="fn-tac fn-fwb">管辖异议二审裁定</td>
                <td><input type="text" class="text" name="jurisdictionObjectionJudgmentSecond" value="$!trailInfo.jurisdictionObjectionJudgmentSecond" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailJurisdictionObjectionJudgmentSecondUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionJudgmentSecondUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailJurisdictionObjectionJudgmentSecondUrl</td>
                <td class="fn-tac fn-fwb">证据交换</td>
                <td><input type="text" class="text" name="evidenceExchange" value="$!trailInfo.evidenceExchange" maxlength="20"></td>
                <td>&nbsp;</td>
            </tr>

            <tr>
                <td class="fn-tac fn-fwb">鉴定申请</td>
                <td><input type="text" class="text" name="appraisalApply" value="$!trailInfo.appraisalApply" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailAppraisalApplyUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailAppraisalApplyUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailAppraisalApplyUrl</td>
                <td class="fn-tac fn-fwb">鉴定材料</td>
                <td><input type="text" class="text" name="appraisalMaterial" value="$!trailInfo.appraisalMaterial" maxlength="20"></td>
                <td>&nbsp;</td>
            </tr>

            <tr>
                <td class="fn-tac fn-fwb">鉴定费用</td>
                <td><input type="text" class="text fnMakeMoney fnMakeMicrometer" name="appraisalAmount" value="$!trailInfo.appraisalAmount" style="width: 80%;">元</td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailAppraisalAmountUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailAppraisalAmountUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailAppraisalAmountUrl</td>
                <td class="fn-tac fn-fwb">申请调查取证</td>
                <td><input type="text" class="text" name="investigatingApply" value="$!trailInfo.investigatingApply" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailInvestigatingApplyUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailInvestigatingApplyUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailInvestigatingApplyUrl</td>
            </tr>

            <tr>
                <td class="fn-tac fn-fwb">申请证人出庭</td>
                <td><input type="text" class="text" name="witnessesApply" value="$!trailInfo.witnessesApply" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailWitnessesApplyUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailWitnessesApplyUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件 $!trailInfo.recoveryLitigationBeforeTrailWitnessesApplyUrl</td>
                <td class="fn-tac fn-fwb">增加诉讼请求申请</td>
                <td><input type="text" class="text" name="increaseLitigationApply" value="$!trailInfo.increaseLitigationApply" maxlength="20"></td>
                <td>
                    #set( $thisUpAttachName = '' )
                    #set( $thisUpAttachValue = '' )

                    #set( $thisUpAttachName = 'recoveryLitigationBeforeTrailIncreaseLitigationApplyUrl' )
                    #set( $thisUpAttachValue = $!trailInfo.recoveryLitigationBeforeTrailIncreaseLitigationApplyUrl )
                    #set( $thisUpAttachConcise = true )

                    #parse( '/layout/common/upAttachDaliog.vm' )
                </td>
                ## <td>这里需要一个上传附件  $!trailInfo.recoveryLitigationBeforeTrailIncreaseLitigationApplyUrl</td>
            </tr>

        </tbody>

    </table>

</div>

<div class="m-item fn-mt20">
    <label class="m-label">备注：</label>
    <textarea class="ui-textarea fn-w700" name="memo">$!trailInfo.memo</textarea>
</div>

<div class="m-item fnUpAttach">
    <label class="m-label">附件上传：</label>
    <a href="javascript:void(0);" class="ui-btn ui-btn-fill ui-btn-blue fnUpAttachBtn"><i class="icon icon-add"></i>上传附件</a>
    <input class="fnUpAttachVal" type="hidden" name="recoveryLitigationBeforeTrailMemoUrl" value="$!trailInfo.recoveryLitigationBeforeTrailMemoUrl">
    <div class="m-attach fnUpAttachUl">
        
        #set( $thisAttachVal = '' ) ## 先制空，再赋值，避免被循环使用
        #set( $thisAttachVal = $!trailInfo.recoveryLitigationBeforeTrailMemoUrl )  ## string 附件的url集合

        #parse('/layout/common/viewAttachUl.vm')

    </div>
</div>