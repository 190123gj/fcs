	<typemap from="byte" to="int" />
	<typemap from="short" to="int" />
	<typemap from="float" to="double" />
	<typemap from="decimal" to="double" />
	<typemap from="java.util.Map"
		to="java.util.List&lt;java.util.Map&lt;String,Object&gt;&gt;" />

