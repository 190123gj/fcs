	            </isNotEmpty>
	            <isNotEmpty property="projectDataInfo.status" prepend=" and ">
	               	status = #projectDataInfo.status#
	            </isNotEmpty>
	        </dynamic>                            				
	         <isNotEmpty property="sortCol" prepend=" order by ">
	             $sortCol$
	                <isNotEmpty property="sortOrder">
              	 		$sortOrder$
             		</isNotEmpty>
	         </isNotEmpty>
            LIMIT #limitStart#,#pageSize#;
	        ]]>
	    </sqlmap>
	</operation>
	
