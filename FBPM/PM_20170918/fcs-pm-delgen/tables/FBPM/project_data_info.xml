	<operation name="findByCondition" paramtype="object" multiplicity="many">
		<extraparams>
			<param name="limitStart" javatype="long" />
			<param name="pageSize" javatype="long" />
			<param name="sortCol" javatype="java.lang.String" />
			<param name="sortOrder" javatype="java.lang.String" />			
		</extraparams>
	    <sql>
	        <![CDATA[
	       		SELECT  * FROM project_data_info WHERE 1=1
	        ]]>
	    </sql>
	    <sqlmap>
	        <![CDATA[
	        SELECT * from project_data_info WHERE 1=1 
	        <dynamic>
	            <isNotEmpty property="projectDataInfo.projectCode" prepend=" and ">
	               	project_code = #projectDataInfo.projectCode#
	            </isNotEmpty>
	            <isNotEmpty property="projectDataInfo.projectName" prepend=" and ">
